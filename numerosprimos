#include <iostream>
#include <vector>
using namespace std;

bool esPrimo(int n) {
    if (n <= 1) return false;
    for (int i = 2; i * i <= n; i++) {
        if (n % i == 0) return false;
    }
    return true;
}

vector<int> primosMenoresQue(int m) {
    vector<int> primos;
    for (int i = 2; i < m; i++) {
        if (esPrimo(i)) {
            primos.push_back(i);
        }
    }
    return primos;
}

int main() {
    int m;
    cout << "Ingrese el valor de m: ";
    cin >> m;
    vector<int> primos = primosMenoresQue(m);
    cout << "NÃºmeros primos menores que " << m << ": ";
    for (int primo : primos) {
        cout << primo << " ";
    }
    cout << endl;
    return 0;
}